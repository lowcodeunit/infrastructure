{
    "parameters": {
        "name": {
            "type": "string"
        },
        "safename": {
            "type": "string"
        },
        "subscriptionId": {
            "type": "string"
        },
        "regionId": {
            "defaultValue": "westus2",
            "type": "string"
        },
        "locationName": {
            "defaultValue": "West US 2",
            "type": "string"
        }
    },
    "resources": [
        {
            "type": "modules",
            "path": "fathym\\core\\insights"
        },
        {
            "type": "modules",
            "path": "fathym\\data-flows\\warm-storage",
            "name": "parameters('name')"
        },
        {
            "type": "modules",
            "path": "fathym\\data-flows\\data-stream",
            "name": "concat(parameters('name'), 'hub')"
        }
    ]
}
